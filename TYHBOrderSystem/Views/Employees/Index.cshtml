        <th>
            @Html.DisplayNameFor(model => model.Emp_Last_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Emp_Initials)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
